{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug Test Suite",
      "type": "node",
      "request": "launch",
      "program": "${workspaceRoot}/nmap-exploit-analyzer.test.js",
      "console": "integratedTerminal",
      "skipFiles": ["<node_internals>/**"]
    },
    {
      "name": "Debug with Node Inspector",
      "type": "node",
      "request": "launch",
      "program": "${workspaceRoot}/nmap-exploit-analyzer.test.js",
      "protocol": "inspector",
      "console": "integratedTerminal"
    },
    {
      "name": "Vite Dev Server",
      "type": "node",
      "request": "launch",
      "program": "${workspaceRoot}/node_modules/vite/bin/vite.js",
      "skipFiles": ["<node_internals>/**"],
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen"
    },
    {
      "name": "Attach to Process",
      "type": "node",
      "request": "attach",
      "port": 9229,
      "skipFiles": ["<node_internals>/**"]
    }
  ]
}

